
<div class="d-flex flex-column w-25">
    <h5>Current Count: @CurrentCount</h5>

    <button class="btn btn-primary mb-4" @onclick="IncrementCount">Number Go Up</button>
    <button class="btn btn-primary" @onclick="DecrementCount">Number Go Down</button>
</div>


@code {
    [Parameter]
    public int CurrentCount { get; set; }

    private void IncrementCount()
    {
        CurrentCount += 666;
    }

    private void DecrementCount()
    {
        CurrentCount--;
    }
}
